<div class="beer-content">
    {{#each randomBeerFromApi}}
        {{> beerpartial this}}
        <h4> Food Pairing: </h4>
        <ul>
            {{#each this.food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p><strong>Brewer Tips:</strong>  {{this.brewers_tips}}</p>
    {{/each}}
</div>